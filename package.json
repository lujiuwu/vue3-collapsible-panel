{
  "name": "vue3-collapsible-panel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "vue-tsc -p tsconfig.app.json --declaration --emitDeclarationOnly --outDir dist/types",
    "build:lib": "npm run build:types && vite build --config vite.lib.config.ts",
    "preview": "vite preview"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@jamescoyle/vue-icon": "^0.1.2",
    "@mdi/js": "^7.4.47",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@unocss/preset-attributify": "^66.5.4",
    "@unocss/preset-icons": "^66.5.4",
    "@unocss/preset-typography": "^66.5.4",
    "@unocss/preset-uno": "^66.5.4",
    "@unocss/preset-web-fonts": "^66.5.4",
    "@unocss/preset-wind": "^66.5.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "unocss": "^66.5.4",
    "vite": "npm:rolldown-vite@7.1.14",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
